@inject MudBlazor.IDialogService DialogService

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    [Parameter] public List<TbOdemeSekli> PaymentTypes { get; set; } = new();

    private string searchTerm = "";
    private List<TbOdemeSekli> FilteredPaymentType =>
        string.IsNullOrWhiteSpace(searchTerm)
        ? PaymentTypes
        : PaymentTypes.Where(m => m.sAciklama.Contains(searchTerm, StringComparison.OrdinalIgnoreCase)).ToList();

    private void SelectMusteri(TbOdemeSekli odemeSekli)
    {
        MudDialog.Close(DialogResult.Ok(odemeSekli));
    }
}

